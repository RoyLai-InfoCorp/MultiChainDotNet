{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {

    "MultiChainDotNet.Api.Service": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost:12018/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://+:12018"
      },
      "dotnetRunMessages": "true",
      "httpPort": 12018
    },

    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://+:12018"
      },
      "httpPort": 12018,
      "publishAllPorts": true
    }
  }
}